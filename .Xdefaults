Xft.dpi: 92
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

urxvt*font: xft:JetBrains Mono:style=Regular:pixelsize=15, xft:Monaco for Powerline:pixelsize=13, xft:Monaco:pixelsize=13
urxvt.utf8: true
urxvt.locale: true
urxvt.letterSpace: 0

urxvt*transparent:true
urxvt*shading:80
urxvt*background:Black

urxvt*foreground: White
urxvt*geometry: 280x224
urxvt*cursorBlink: true
urxvt*cursorColor: #5eff24
urxvt*saveLines: 20000
urxvt*scrollBar:         false

! Extensions
URxvt.perl-lib: /home/gogoprog/scripts
URxvt.perl-ext: default,matcher,mark-yank-urls
URxvt.underlineURLs: true
URxvt.keysym.C-i: perl:mark-yank-urls:activate_mark_mode
URxvt.urlLauncher: /usr/bin/xdg-open
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline

! Keys
urxvt.keysym.C-Tab: \033{
!urxvt.keysym.Control-i: \033}
!urxvt.keysym.Shift-ISO_Left_Tab: \033}
urxvt.keysym.Control-Shift-F: \033]
urxvt*iso14755: False
urxvt*iso14755_52: False

URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
