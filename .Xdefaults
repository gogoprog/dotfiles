Xft.dpi: 92
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

urxvt*transparent:true
urxvt*shading:40
urxvt*background:Black

urxvt*foreground: White
urxvt*geometry: 80x24
urxvt*cursorBlink: true
urxvt*cursorColor: #5eff24
urxvt*saveLines: 20000
urxvt*scrollBar:         false

! Extensions
URxvt.perl-lib: /home/gogoprog/scripts
URxvt.perl-ext: mark-yank-urls
URxvt.underlineURLs: true
URxvt.matcher.rend.0: fg2
URxvt.keysym.C-u: perl:mark-yank-urls:activate_mark_mode
URxvt*urlLauncher: chromium

urxvt*font: xft:Monaco:pixelsize=11

urxvt.keysym.C-Tab: \033{
!urxvt.keysym.Control-i: \033}
!urxvt.keysym.Shift-ISO_Left_Tab: \033}
urxvt.keysym.Control-Shift-F: \033]
urxvt*iso14755: False
urxvt*iso14755_52: False
